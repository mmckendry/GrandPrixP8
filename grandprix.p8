version 29
__lua__

 function _init()
  screenwidth = 127
  screenheight = 127
  make_player()
  scene=0
  lap=0
  timer=0
  delay=0
  shake=0
  display=false
  is_generated=false
  first=true
 end
 
 function _update()
  if(scene==0) then
   title_update()
  elseif (scene==1) then
   move_player()
   if (player["fuel"]!=0) then 
    pathfinding(player)
    if(delay > 1) then 
     pathfinding(opponent)
    end
   end
  elseif(scene==2) then
   finish_update()
  elseif(scene==3) then
   pitstop_update()
  end
 end
 
 function _draw()
  cls()
  if(scene==0) then
   title_draw()
  elseif(scene==1) then
   camera_follow()
   drawtrack()
   spr(0,player[2]["cx"]*8,player[2]["cy"]*8)
   spr(2,opponent[2]["cx"]*8,opponent[2]["cy"]*8)
   update_fuel()
   display_fuel()
   metrics()
   out_of_fuel()
   display_textwindow()
  elseif(scene==2) then 
   finish_draw()
  elseif(scene==3) then
   pitstop_draw()
  end
end

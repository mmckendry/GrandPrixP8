--utility functions
c={}
c.x=0
c.y=0
--camera follows the player
function camera_follow()
 c.x=player[2]["cx"]-10
 c.y=player[2]["cy"]-10

 c.x=mid(0,c.x,128)
 c.y=mid(0,c.y,128)

 camera(c.x*8,c.y*8)
end

function camera_shake()
 local shakex=16-rnd(32)
 local shakey=16-rnd(32)
 shakex*=shake
 shakey*=shake
 
 camera(shakex,shakey)
 shake = shake*0.95

 if (shake<0.05) then 
  shake=0
 end
end

--collision detection
function can_move(x,y)
 local map_sprite=mget(x,y)
 local flag=fget(map_sprite)
 
 if(flag==4)then 
  scene=3
 end
 if(flag==2) then
  lap+=1 
  if(lap==4 and flag==2) then
    scene=2
  end
 end
 return flag!=1
end

--update fuel
function update_fuel()
 if(delay<60) then 
  delay+=1
 end
 if(delay>=60) then
  delay=0
  if (player.fuel!=0) then
   player["fuel"]-=1
  end
 end
end

--pathfinding for the player
function pathfinding(car)
 local up={}
 local down={}
 local left={}
 local right={}
 local neighbours={}
 local candidates={}
 local next_step={}

 car[3]["nx"]=car[2]["cx"] 
 car[3]["ny"]=car[2]["cy"]
 if(first)then 
  car[1]["lx"]-=1 
  car[1]["ly"]=car[2]["cy"]
  first=false
 end

function set_neighbours()
  left[1]=car[3]["nx"] - 1
  left[2]=car[3]["ny"]
  right[1]=car[3]["nx"] + 1
  right[2]=car[3]["ny"]
  up[1]=car[3]["nx"]
  up[2]=car[3]["ny"] + 1
  down[1]=car[3]["nx"]
  down[2]=car[3]["ny"] - 1
  neighbours={left,right,up,down}
 end
 
 set_neighbours()
 
  for k, v in ipairs(neighbours) do
   for j, m in ipairs(v) do
     candidates[j]=m 
   end
   if (candidates[1]!=car[1]["lx"] or candidates[2]!=car[1]["ly"]) then
    -- sfx(0) 
     if(can_move(candidates[1], candidates[2])) then
      next_step[1]=candidates[1]
      next_step[2]=candidates[2]
     end
   end
  end
  
 car[1]["lx"]=car[2]["cx"]
 car[1]["ly"]=car[2]["cy"]
  
 car[2]["cx"]=next_step[1]
 car[2]["cy"]=next_step[2]
end

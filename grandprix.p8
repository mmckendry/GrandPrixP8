  end
  if(btnp(‚¨ÖÔ∏è))then
   check_sequence("‚¨ÖÔ∏è")
  end
 end   
end

function generate_sequence()
 if(is_generated==false) then
  for i=1,4 do
   k=flr(rnd(4))
   if k==0 then
    k="‚¨ÜÔ∏è"
   elseif k==1 then
    k="‚û°Ô∏è"
   elseif k==2 then
    k="‚¨áÔ∏è"
   elseif k==3 then
    k="‚¨ÖÔ∏è"
  end
 s[i]["v"]=k
 end
end
  
 print(s[1]["v"],45,65,s[1]["c"])
 print(s[2]["v"],55,65,s[2]["c"])
 print(s[3]["v"],65,65,s[3]["c"])
 print(s[4]["v"],75,65,s[4]["c"])
 print("timer: "..timer/30,45,25,s[4]["c"])
 is_generated=true
end

function check_sequence(button)
 if(s_finished==false) then
  if (shake<0.1000) then
   if(s[s_count]["v"]==button) then 
    s[s_count]["c"]=9
    s_count+=1
    if(s_count==5) then
     s_finished=true
     s_count=1
    end
   else
    shake+=1
   end
  end
 end
end

function reset_values()
 for i=1,4 do 
  s[i]["c"]=8
 end
 s_count=1 
 scene=1
 s_finished=false
 timer=0
 player["fuel"]=10
end

function pit_timer()
 if(s_finished==false) then
  timer+=1
 end
end 
